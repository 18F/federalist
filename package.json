  "scripts": {
    "postinstall": "npm run uswds && npm run migrate:up",
    "build": "webpack -p",
    "start": "node index.js",
    "debug": "node debug index.js",
    "watch": "npm run uswds && node index.js & npm run watch:client",
    "watch:server": "nodemon index.js --ignore frontend/ --ignore .tmp/ --ignore node_modules/",
    "watch:client": "webpack --progress --colors --watch",
    "test": "npm run test:prepare && npm run test:server && npm run test:client",
    "test:prepare": "NODE_ENV=test npm run migrate:up && npm run build",
    "test:server": "NODE_ENV=test mocha --timeout 1000 test/api/bootstrap.test.js test/api/unit/**/*.test.js test/api/requests/**/*.test.js",
    "test:client": "mocha --compilers js:babel-register --recursive ./test/frontend",
    "test:client:watch": "watch 'npm run test:client' ./frontend ./test/frontend",
    "coverage:client": "istanbul cover _mocha -- --compilers js:babel-register --recursive ./test/frontend",
    "lint": "eslint ./api/**/*.js ./config/**/*.js ./frontend/**/*.js ./migration/**/*.js ./script/**/*.js ./services/**/*.js",
    "lint:diff": "./scripts/lint-diff.sh",
    "migrate:create": "node migrate.js create",
    "migrate:up": "node migrate.js up || true",
    "migrate:down": "node migrate.js down",
    "uswds": "if [ -d ./node_modules/uswds ]; then npm run uswds:css && npm run uswds:images && npm run uswds:fonts; fi",
    "uswds:css": "cp node_modules/uswds/dist/css/uswds.min.css public/styles/",
    "uswds:images": "cp -r node_modules/uswds/dist/img/ ./public/images/uswds",
    "uswds:fonts": "cp node_modules/uswds/dist/fonts/* ./public/fonts",
    "export:sites": "node ./scripts/exportSitesAsCsv.js"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/18f/federalist.git"
  },
  "author": "dhcole",
  "license": "Public Domain",
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.16.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "cookie": "^0.3.1",
    "css-loader": "^0.28.4",
    "enzyme": "^2.4.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "istanbul": "^1.1.0-alpha.1",
    "json-schema-deref-sync": "^0.3.3",
    "jsonschema": "^1.1.1",
    "mocha": "2.3.4",
    "nock": "^9.0.2",
    "node-sass": "^4.5.3",
    "nodemon": "^1.3.7",
    "postcss-loader": "^2.0.5",
    "promise-props": "^1.0.0",
    "proxyquire": "^1.7.10",
    "react-addons-test-utils": "^15.3.1",
    "react-test-renderer": "^15.5.4",
    "redux": "^3.5.2",
    "sass-loader": "^6.0.5",
    "sinon": "1.17.2",
    "supertest": "^2.0.1",
    "supertest-as-promised": "^4.0.1",
