  "scripts": {
    "install": "npm run uswds && npm run codemirror && npm run migrate:up",
    "build": "npm run browserify && npm run sass && npm run autoprefixer",
    "dev:build": "npm run dev:browserify && npm run sass && npm run autoprefixer",
    "start": "npm run build && node app.js",
    "dev:start": "npm run dev:build && node app.js",
    "debug": "node debug app.js",
    "browserify": "browserify assets/app/main.js -o assets/js/bundle.js -t [ babelify --presets [ es2015 react ] ]",
    "dev:browserify": "NODE_ENV=development browserify --debug assets/app/main.js -o assets/js/bundle.js -t [ babelify --presets [ es2015 react ] ]",
    "sass": "node-sass assets/styles/styles.scss -o assets/styles",
    "autoprefixer": "postcss --use autoprefixer -r assets/styles/styles.css",
    "watch": "npm run uswds && node app.js & npm run watch:client",
    "watch:server": "nodemon app.js --ignore assets/app --ignore .tmp/ --ignore node_modules/",
    "watch:client": "npm run watch:client:sass & npm run watch:client:autoprefix & npm run watch:client:js",
    "watch:client:sass": "node-sass -w -r assets/styles/*.scss -o assets/styles",
    "watch:client:autoprefix": "npm run sass && postcss --use autoprefixer -r -w assets/styles/styles.css assets/styles/styles.css",
    "watch:client:js": "watchify assets/app/main.js -o assets/js/bundle.js -t [ babelify --presets [ es2015 react ] ]",
    "test": "npm run build && npm run test:server && npm run test:client",
    "test:server": "NODE_ENV=test mocha --timeout 60000 test/api/bootstrap.test.js test/api/unit/**/*.test.js",
    "test:integration": "NODE_ENV=test mocha test/integration/bootstrap.test.js \"test/integration/**/*.test.js\"",
    "test:selenium:install": "node_modules/selenium-standalone/bin/selenium-standalone install",
    "test:selenium:start": "node_modules/selenium-standalone/bin/selenium-standalone start",
    "test:client": "mocha --compilers js:babel-register --recursive ./test/client",
    "test:client:watch": "watch 'npm run test:client' ./assets/app ./test/client",
    "coverage:client": "istanbul cover _mocha -- --compilers js:babel-register --recursive ./test/client",
    "lint": "eslint ./**",
    "migrate:create": "grunt db:migrate:create",
    "migrate:up": "grunt db:migrate:up || true",
    "migrate:down": "grunt db:migrate:down",
    "uswds": "npm run uswds:css && npm run uswds:images && npm run uswds:fonts",
    "uswds:css": "cp node_modules/uswds/dist/css/uswds.css assets/styles/",
    "uswds:images": "cp -r node_modules/uswds/dist/img/ ./assets/images/uswds",
    "uswds:fonts": "cp node_modules/uswds/dist/fonts/* ./assets/fonts",
    "codemirror": "cp node_modules/codemirror/lib/codemirror.css assets/styles/",
    "export:sites": "node ./scripts/exportSitesAsCsv.js"
  },
  "main": "app.js",
