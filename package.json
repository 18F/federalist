{
  "name": "federalist",
  "private": true,
  "version": "0.0.0",
  "description": "",
  "keywords": [],
  "dependencies": {
    "aws-sdk": "^2.1.36",
    "bluebird": "^3.4.6",
    "body-parser": "^1.16.1",
    "cfenv": "^1.0.0",
    "connect-session-sequelize": "^4.1.0",
    "core-js": "^2.4.1",
    "db-migrate": "^0.9.17",
    "deep-extend": "^0.4.1",
    "express": "^4.14.1",
    "express-session": "^1.5.0",
    "express-winston": "^2.3.0",
    "github": "^0.2.4",
    "method-override": "^2.3.7",
    "moment": "^2.10.3",
    "newrelic": "^1.22.0",
    "passport": "^0.2.1",
    "passport-github": "^0.1.5",
    "pg": "^4.4.0",
    "rc": "~0.5.0",
    "request": "^2.61.0",
    "sequelize": "^3.30.0",
    "underscore": "^1.8.3",
    "urlsafe-base64": "^1.0.0",
    "whatwg-fetch": "^1.0.0",
    "winston": "^2.3.1",
    "yamljs": "^0.2.8"
  },
  "scripts": {
    "postinstall": "npm run uswds && npm run migrate:up",
    "build": "npm run browserify && npm run sass && npm run autoprefixer",
    "start": "node index.js",
    "debug": "node debug index.js",
    "browserify": "browserify frontend/main.js -o public/js/bundle.js -t [ babelify --presets [ es2015 react stage-2 ] ]",
    "sass": "node-sass public/styles/styles.scss -o public/styles",
    "autoprefixer": "postcss --use autoprefixer -r public/styles/styles.css",
    "watch": "npm run uswds && node index.js & npm run watch:client",
    "watch:server": "nodemon index.js --ignore frontend/ --ignore .tmp/ --ignore node_modules/",
    "watch:client": "npm run watch:client:sass & npm run watch:client:autoprefix & npm run watch:client:js",
    "watch:client:sass": "node-sass -w -r public/styles/*.scss -o public/styles",
    "watch:client:autoprefix": "npm run sass && postcss --use autoprefixer -r -w public/styles/styles.css public/styles/styles.css",
    "watch:client:js": "watchify frontend/main.js -o public/js/bundle.js -t [ babelify --presets [ es2015 react stage-2 ] ]",
    "test": "npm run test:prepare && npm run test:server && npm run test:client",
    "test:prepare": "NODE_ENV=test npm run migrate:up && npm run build",
    "test:server": "NODE_ENV=test mocha --timeout 6000 test/api/bootstrap.test.js test/api/unit/**/*.test.js test/api/requests/**/*.test.js",
    "test:client": "mocha --compilers js:babel-register --recursive ./test/frontend",
    "test:client:watch": "watch 'npm run test:client' ./frontend ./test/frontend",
    "coverage:client": "istanbul cover _mocha -- --compilers js:babel-register --recursive ./test/frontend",
    "lint": "eslint ./**",
    "migrate:create": "node migrate.js create",
    "migrate:up": "node migrate.js up || true",
    "migrate:down": "node migrate.js down",
    "uswds": "if [ -d ./node_modules/uswds ]; then npm run uswds:css && npm run uswds:images && npm run uswds:fonts; fi",
    "uswds:css": "cp node_modules/uswds/dist/css/uswds.min.css public/styles/",
    "uswds:images": "cp -r node_modules/uswds/dist/img/ ./public/images/uswds",
    "uswds:fonts": "cp node_modules/uswds/dist/fonts/* ./public/fonts",
    "export:sites": "node ./scripts/exportSitesAsCsv.js"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/18f/federalist.git"
  },
  "author": "dhcole",
  "license": "Public Domain",
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "aws-sdk-mock": "^1.5.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.16.0",
    "babel-register": "^6.9.0",
    "babelify": "^7.3.0",
    "browserify": "^10.2.4",
    "chai": "^3.5.0",
    "cookie": "^0.3.1",
    "enzyme": "^2.4.1",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^6.2.0",
    "istanbul": "^1.1.0-alpha.1",
    "json-schema-deref-sync": "^0.3.3",
    "json-to-csv": "^1.0.0",
    "jsonschema": "^1.1.1",
    "mocha": "2.3.4",
    "nock": "^9.0.2",
    "node-sass": "^3.4.2",
    "nodemon": "^1.3.7",
    "postcss-cli": "^2.3.2",
    "proxyquire": "^1.7.10",
    "react": "^15.1.0",
    "react-addons-test-utils": "^15.3.1",
    "react-dom": "^15.1.0",
    "react-router": "^2.4.1",
    "redux": "^3.5.2",
    "sinon": "1.17.2",
    "supertest": "^2.0.1",
    "supertest-as-promised": "^4.0.1",
    "uswds": "^0.9.0",
    "watch": "^0.19.2",
    "watchify": "^3.2.2"
  },
  "engines": {
    "node": "~6.9.0",
    "npm": "~3.10.8"
  }
}
