{
  "type": "object",
  "required": [
    "id",
    "defaultBranch",
    "engine",
    "owner",
    "repository",
    "users"
  ],
  "properties": {
    "id": {
      "type": "integer"
    },
    "demoBranch": {
      "type": "string"
    },
    "demoDomain": {
      "type": "string"
    },
    "config": {
      "type": "string"
    },
    "defaultBranch": {
      "type": "string"
    },
    "domain": {
      "type": "string"
    },
    "engine": {
      "type": "string",
      "enum": ["jekyll", "hugo", "static"]
    },
    "owner": {
      "type": "string"
    },
    "previewConfig": {
      "type": "string"
    },
    "demoConfig": {
      "type": "string"
    },
    "publishedAt": {
      "type": "string",
      "format": "date-time"
    },
    "repository": {
      "type": "string"
    },
    "users": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["id", "username", "email"],
        "not": {
          "required": ["githubAccessToken", "githubUserId"]
        }
      }
    }
  }
}
